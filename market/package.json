{
  "name": "market",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "GENERATE_SOURCEMAP=false nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export MONGO_DEV_ENV=local && npm run test:docker:up && nest start --watch",
    "start:dev:mongo-atlas": "export MONGO_DEV_ENV=atlas && npm run test:docker:up:postgres && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pm2:dev": "pm2 startOrRestart ecosystem.config.js --only pfa_market_dev --env development",
    "pm2:prod": "pm2 startOrRestart ecosystem.config.js --only pfa_market_prod --env production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:docker:up": "docker compose -p pfa-market-test --profile test -f test/docker-compose.yml up -d",
    "test:docker:up:postgres": "docker compose -p pfa-market-test --profile postgres -f test/docker-compose.yml up -d",
    "test:docker:stop": "docker compose -p pfa-market-test --profile test -f test/docker-compose.yml stop",
    "test:docker:down": "docker compose -p pfa-market-test --profile test -f test/docker-compose.yml down",
    "test:e2e:docker:build:child": "docker build ../marketChild -t pfa-market-child",
    "test:e2e:docker:up": "docker compose -p pfa-market-test --profile e2e -f test/docker-compose.yml up -d",
    "test:e2e:docker:stop": "docker compose -p pfa-market-test --profile e2e -f test/docker-compose.yml stop",
    "test:e2e:docker:down": "docker compose -p pfa-market-test --profile e2e -f test/docker-compose.yml down",
    "test": "npm run test:docker:up && jest",
    "test:watch": "npm run test:docker:up && jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export MONGO_DEV_ENV=local && npm run test:e2e:docker:up && jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource typeormDataSource/postgres.ts",
    "typeorm:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource typeormDataSource/postgres.testdb.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create migrations/postgres/Migration",
    "migration:generate": "npm run typeorm migration:generate migrations/postgres/Migration",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:test::run": "npm run typeorm:test migration:run",
    "migration:test:revert": "npm run typeorm:test migration:revert",
    "seeder:simple:dev:20240513": "ts-node -r tsconfig-paths/register seeder/simple/dev.20240513.ts"
  },
  "dependencies": {
    "@fxts/core": "^0.12.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "helmet": "^7.1.0",
    "mongoose": "^6.5.2",
    "pg": "^8.11.2",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
